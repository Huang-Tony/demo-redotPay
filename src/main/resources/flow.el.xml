<?xml version="1.0" encoding="UTF-8"?>
<flow>
    <!-- Dynamic condition chain -->
    <chain name="dynamicConditionChain">
        THEN(dynamicConditionCmp, printResultCmp)
    </chain>

    <!-- Keep existing chains for backward compatibility -->
    <chain name="greaterThan5Chain">
        THEN(numberCheckCmp, printResultCmp)
    </chain>

    <chain name="integerCheckChain">
        THEN(integerCheckCmp, printResultCmp)
    </chain>
</flow>